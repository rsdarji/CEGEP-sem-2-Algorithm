def fibonacci_sequence(indexNumber):

    a,b = 0,1
    lst = []
    if(indexNumber>=1):
        lst.append(0)
        a=b
        b=lst[0]
    for i in range(1,indexNumber):
        lst.append(a+b)
        a=b
        b=lst[i]
    return lst

indexNumber = int(input("Please enter a series length : "))
fibonacciSeries = fibonacci_sequence(indexNumber)

